{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    ".eslintrc.js",
    ".prettierrc.js",
    "babel.config.js",
    "expo-env.d.ts"
  ]
}
