{
  "name": "unistyles-sandbox",
  "main": "index",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:prod": "expo run:ios --configuration Release",
    "prebuild": "expo prebuild --clean",
    "lint": "yarn lint:tsc && yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "node_modules/.bin/eslint --fix '**/*.{ts,tsx,js}'",
    "lint:prettier": "prettier --write \"**/*.{md,ts,tsx,js,json}\"",
    "lint:tsc": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "expo:check": "npx expo install --check && npx expo-doctor"
  },
  "dependencies": {
    "expo": "53.0.12",
    "expo-build-properties": "0.14.6",
    "expo-constants": "17.1.6",
    "expo-font": "13.3.1",
    "expo-linking": "7.1.5",
    "expo-router": "5.1.0",
    "expo-splash-screen": "0.30.9",
    "expo-status-bar": "2.2.3",
    "expo-symbols": "0.4.5",
    "expo-system-ui": "5.0.9",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-gesture-handler": "2.24.0",
    "react-native-nitro-modules": "0.26.2",
    "react-native-reanimated": "3.18.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.11.1",
    "react-native-unistyles": "3.0.0-nightly-20250617"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@hello-aurora/eslint-config-pretty": "0.2.0",
    "@types/react": "19.0.10",
    "typescript": "5.8.3"
  }
}
